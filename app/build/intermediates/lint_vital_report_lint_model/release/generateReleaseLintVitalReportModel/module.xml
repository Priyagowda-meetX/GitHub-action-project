<lint-module
    format="1"
    dir="/root/GitHub-action-project/app"
    name=":app"
    type="APP"
    maven="AndroidCICD:app:unspecified"
    agpVersion="8.8.0"
    buildFolder="build"
    bootClassPath="/root/android-sdk/platforms/android-35-2/android.jar:/root/android-sdk/build-tools/35.0.0/core-lambda-stubs.jar"
    javaSourceLevel="11"
    compileTarget="android-35"
    neverShrinking="true">
  <lintOptions
      abortOnError="true"
      absolutePaths="true"
      checkReleaseBuilds="true"
      explainIssues="true"/>
  <variant name="release"/>
</lint-module>
